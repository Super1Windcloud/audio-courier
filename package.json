{
  "name": "audio-courier",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "author": "superwindcloud",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "start": "pnpm tauri dev",
    "release": "pnpm build && pnpm tauri build",
    "clean": "cd src-tauri && cargo clean",
    "check": "biome check src  --write",
    "lint": "eslint  src  --fix --max-warnings=0",
    "format": "pnpm --stream --color \"/format:/\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "format:js": "biome format src  --write",
    "generate:icons": "tauri icon  app-icon.png",
    "generate:signkey": "tauri signer generate -w .tauri/${npm_package_name}_signer.key",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.0.4"
  }
}
