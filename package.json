{
  "name": "audio-courier",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "author": "superwindcloud",
  "license": "Apache-2.0",
  "homepage": "https://github/super1windcloud/audio-courier",
  "scripts": {
    "dev": "vite",
    "taze": "npx taze latest -w",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "td": "pnpm tauri dev",
    "tb": "pnpm tauri build",
    "dry:tb": "pnpm  tauri build --no-bundle",
    "release": "pnpm build && pnpm tauri build",
    "clean": "cd src-tauri && cargo clean",
    "check": "biome check src  --write",
    "lint": "eslint  src  --fix --max-warnings=0",
    "format": "pnpm --stream --color \"/format:/\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "format:js": "biome format src  --write",
    "generate:icons": "tauri icon  app-icon.png",
    "generate:signkey": "tauri signer generate -w .tauri/${npm_package_name}_signer.key",
    "prepare": "husky",
    "migrate": "biome migrate eslint --write",
    "fix": "biome check --write"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-global-shortcut": "~2.3.1",
    "@tauri-apps/plugin-opener": "^2.5.2",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "openai": "^6.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "rm": "^0.1.8",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tauri-apps/cli": "^2.9.4",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "ws": "^8.18.3"
  }
}
